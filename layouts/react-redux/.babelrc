{
    "presets": ["react", "es2015-node5"],
    "plugins": [
        "transform-object-rest-spread",
        "transform-class-properties",
        "add-module-exports",
        "transform-inline-environment-variables",
        [
            "transform-async-to-module-method",
            { "module": "bluebird", "method": "coroutine" }
        ]
    ]
}
