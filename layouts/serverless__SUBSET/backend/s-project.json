{
  "name": "untitled",
  "custom": {
      "webpack": {
        "configPath": "./webpack.config.js"
    },
    "client": {
        "bucketName": "client.${name}"
    },
    "cors": {
      "allowOrigin": "*",
      "allowHeaders": [
        "Content-Type",
        "X-Amz-Date",
        "Authorization",
        "X-Api-Key"
      ]
    }
  },
  "plugins": [
    "serverless-webpack-plugin",
    "serverless-client-s3",
    "serverless-cors-plugin"
  ]
}
